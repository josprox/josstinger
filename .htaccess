<IfModule mod_rewrite.c>
Options +FollowSymLinks
RewriteEngine on
AddDefaultCharset utf-8

RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}.php -f
RewriteRule ^(.*)$ $1.php
#RewriteRule ^([a-z]+)/?$ $1.php [NC]


RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}.html -f
RewriteRule ^(.*)$ $1.html

ErrorDocument 404 /public/document_errors/404.html
ErrorDocument 403 /public/document_errors/404.html
ErrorDocument 410 /public/document_errors/410.html
ErrorDocument 500 /public/document_errors/500.html

</IfModule>

<IfModule mod_expires.c>
